{
  "name": "@enterprise_search/searchclient",
  "description": "A sample search client using these utilities",
  "version": "0.0.1",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^3.28.0",
    "@enterprise_search/authentication": "0.0.1",
    "@enterprise_search/confluence_data_plugin": "0.0.1",
    "@enterprise_search/errors": "0.0.1",
    "@enterprise_search/icons": "0.0.1",
    "@enterprise_search/jira_data_plugin": "0.0.1",
    "@enterprise_search/msal_authentication": "0.0.1",
    "@enterprise_search/people_data_plugin": "0.0.1",
    "@enterprise_search/renderers": "0.0.1",
    "@enterprise_search/html_renderers": "0.0.1",
    "@enterprise_search/markdown_renderers": "0.0.1",
    "@enterprise_search/sovereign_mui": "0.0.1",
    "@enterprise_search/search_important_components": "0.0.1",
    "@enterprise_search/selectable_button": "0.0.1",
    "@enterprise_search/sovereign": "0.0.1",
    "@enterprise_search/sovereign_search": "0.0.1",
    "@enterprise_search/service_caller": "0.0.1",
    "@enterprise_search/search_state": "0.0.1",
    "@enterprise_search/react_utils": "0.0.1",
    "@enterprise_search/simple_translation": "0.0.1"
  },
  "devDependencies": {
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^13.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "jest-localstorage-mock": "^2.4.26"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "files": [
    "build/",
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/",
    "build/"
  ],
  "keywords": [
    "react",
    "runbook"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": "git@github.com:intellimaintain/intellimaintain.git",
  "scripts": {
    "8080": "parcel src/index.html --port 8080",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  }
}