{
  "packageManager": "yarn",
  "parents": [
    "@laoban@/laoban.json/core.laoban.json",
    "@laoban@/laoban.json/javascript.laoban.json",
    "@laoban@/laoban.json/typescript.laoban.json",
    "@laoban@/laoban.json/typescript.publish.laoban.json",
    "@laoban@/laoban.json/react.laoban.json"
  ],
  "properties": {
    "axios": "1.7.2",
    "msal": "^3.28.0",
    "license": "MIT",
    "repository": "git@github.com:intellimaintain/intellimaintain.git",
    "laobanVersion": "1.4.51",
    "focusonVersion": "1.44.3",
    "itsmworkbench": "0.3.12",
    "xlsx": "^0.18.5",
    "cheerio": "^1.0.0-rc.12",
    "openapi": "4.58.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "xml2js": "^0.6.2"
  },
  "templates": {
    "typescript": "templates/typescript",
    "react": "templates/react",
    "react_library": "templates/react_library"
  },
  "defaultEnv": {
    "NPM_TOKEN": ""
  },
  "scripts": {
    "coverage": {
      "description": "Runs the coverage for the  projects marked as 'test'",
      "inLinksOrder": true,
      "commands": [
        {
          "guard": "${packageDetails.guards.test}",
          "command": "yarn run test --coverage --coverageDirectory=coverage"
        }
      ]
    },
    "coverage-copy": {
      "description": "Copies the data to the coverage directory",
      "inLinksOrder": true,
      "commands": [
        "file:mkdir(${laobanDirectory}/ncoverage)",
        {
          "guard": "${packageDetails.guards.test}",
          "command": "copy coverage\\coverage-final.json ${laobanDirectory}\\coverage\\${packageDetails.name|forwardSlashToDot}.json"
        }
      ]
    },
    "ls-react": {
      "description": "Lists all the react projects",
      "commands": [
        {
          "guard": "${packageDetails.guards.react}",
          "command": "js:process.cwd()"
        }
      ]
    },
    "ls-build": {
      "description": "Lists all the  projects that will be built",
      "commands": [
        {
          "guard": "${packageDetails.guards.react}",
          "command": "js:process.cwd()"
        }
      ]
    },
    "build": {
      "description": "Packages the react projects",
      "commands": [
        {
          "guard": "${packageDetails.guards.react}",
          "command": "yarn run build"
        }
      ]
    }
  }
}
