{
  "name": "${packageDetails.name}",
  "description": "${packageDetails.description}",
  "version": "${version}",
  "license": "${properties.license}",
  "repository": "${properties.repository}",
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/jest": "^27.5.2"
  },
  "keywords": [
    "indexing"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "test": "jest --config jest.config.json --env node"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
